
.content-wrapper {background: #fefdcc;}

.skin-yellow .main-header .navbar, .skin-yellow .main-header .logo {background-color: #fefa75;}
.skin-yellow .main-header .navbar .nav > li > a { color: #000; }
.skin-yellow .main-header .navbar .nav > li > a:hover { background: none; color: #000; }
.skin-yellow .main-header .navbar .sidebar-toggle { color: #000; }

.skin-yellow .main-sidebar, .skin-yellow .left-side {background-color: #000;}

.skin-yellow .main-header .logo{ color: #000;}
.skin-yellow .main-header .logo:hover, .skin-yellow .main-header .navbar .sidebar-toggle:hover { background-color: #fefa75; color: #000;}

.skin-yellow .sidebar-menu > li.active > a, .skin-yellow .sidebar-menu > li > a:hover {border-left-color: #fefa75;}

.skin-yellow .wrapper, .skin-yellow .main-sidebar, .skin-yellow .left-side { background-color: #000; }


.sidebar-menu { padding: 0 10px; }

.skin-yellow .sidebar-menu > li  { margin: 0 0 5px;}
.skin-yellow .sidebar-menu > li > a {border: 0px;}
.sidebar-menu > li > a {padding: 12px 5px 12px 10px;  display: block;}

.skin-yellow .sidebar-menu > li.active > a,
.skin-yellow .sidebar-menu > li > a:hover,
.skin-yellow .sidebar-menu > li > a:focus {
    background: #35404d;
    color: #fff;
    display: block;
    border-radius: 4px;
    -webkit-border-radius: 4px;
    -webkit-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    -o-transition: all 0.3s ease;
    -ms-transition: all 0.3s ease;
    transition: all 0.3s ease;
    border: 0px;
}

.skin-yellow .sidebar-menu > li > a:hover i { color: #ff6c60; }
.skin-yellow .sidebar-menu > li > .treeview-menu { background: #35404d;  border-radius: 0 0 4px 4px; padding: 8px 0 4px; margin: -4px 0 0 0;}



.main-footer {background: #c12b2b;   color: #fefa75;    border-top: 0px ; bottom: 0; }
.main-footer strong { font-weight: normal; }

.panel-default > .panel-heading {color: #fff;   background-color: #222; }


#progressbar li.active:before, #progressbar li.active:after {background: #222!important;}

#msform .action-button {background: #333!important;}
#msform .action-button:hover, #msform .action-button:focus { box-shadow: 0 0 0 2px white, 0 0 0 3px #333!important;}



.data_info { margin-bottom: 20px; }
.data_info table, .imp_tble { width: 100%!important;  }
.data_info table tr th, .data_info table tr td { padding: 8px; vertical-align: top; border: 1px solid #000; }
.data_info table tr th { box-shadow: none;  }


.loader { display: none; background: #000; opacity: 0.5; position: fixed; width: 100%; height: 100%; z-index: 10; }   
.loader_txt { position: absolute;  top: 50%;  left: 50%;  font-size: 50px;  color: white;  transform: translate(-50%,-50%);  -ms-transform: translate(-50%,-50%); } 

.navbar-nav > .user-menu > .dropdown-menu.cust_dp_menu  { width: 100%; } 
.dropdown-toggle i { margin-right: 5px; }



/*=== login page start===*/

.login-logo {background: #fefa75; padding: 15px; margin: 0px; }
.login-page {background: #fffdcd;}
.login-box-body {background: #f5e65b; }
.login-box-body {box-shadow:  none; }
.login-box {box-shadow: 6px 6px 12px #d8d8d8; margin-top: 100px;}
.btn-login {font-size: 17px; background: #222; padding: 8px; color: #fff; text-transform: uppercase; font-weight: 400;  box-shadow: 0 4px #000; transition: all 0.7s ease; width: 100%; border-radius: 0px;}
.btn-login:hover {color: #666;}

p.login-box-msg { padding: 15px; color: #000; }

/*=== login page end===*/

