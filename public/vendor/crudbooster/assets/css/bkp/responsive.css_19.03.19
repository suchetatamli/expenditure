@media (max-width: 1199px) {}




@media (max-width: 991px) {

.navbar-nav > .user-menu > .dropdown-menu.cust_dp_menu { right: 0; }
.content-header > h1 { font-size: 20px; }

.frm-panel .col-sm-2{ width: 40%; }

.actn-btn-hldr { position: relative; padding: 0 90px 10px 0; min-height: 35px; }
.actn-btn-hldr #btn_add_new_data { position: absolute; right: 0; top: 0; }

.acknowledgement table { width: 800px; }

.content { padding-bottom: 110px; }

}




@media (max-width: 767px) {

.skin-yellow .main-header .navbar .nav .open > a, .skin-yellow .main-header .navbar .nav .open > a:focus {background: rgba(0, 0, 0,1);}



.login-box { margin-top: 30px;}


.user_info_header, .main-header .navbar-custom-menu { width: auto; float: none; }

.user_info_header { padding-right: 60px; }

.party_logo_header { left: auto; right: 15px; margin-left: 0px; }


.top_date .row .col-sm-4:last-child label { display: none; }

.content-header > h1 { font-size: 16px; }

.content { padding-bottom: 20px; }

body.sidebar-collapse .main-footer, .main-footer {width: 100%;}
.main-footer { padding: 10px 15px; position: relative; }


h1 span.link-conte-span{ top: 0px; right: 0px; position: relative; display: block; padding: 10px 0;}
.content-header > h1 span.header-text-span { width: auto;}

.frm-panel .col-sm-2{ width: auto; }

.annexure_tbl { margin-bottom: 10px; }
.annexure_tbl table { width: 800px; }

.guidlines { float: none; padding-top: 10px; display: block; }

h1.actn-btn-hldr .icon_b { margin-top: 0px; }

.dd-select, .dd-option-text { font-size: 13px; }

}



@media (max-width: 639px) {}



@media (max-width: 479px) {
.user_info_header .user_des .user_txt span { font-size: 13px; }



}

